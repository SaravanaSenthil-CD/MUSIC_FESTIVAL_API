{
    "extends": [
      "tslint:recommended",
      "tslint-config-prettier"
    ],
    "rules": {
      "no-console": false,
      "no-debugger": false,
      "max-line-length": [true, 120],
      "indent": [true, "spaces", 2],
      "quotemark": [true, "single"],
      "interface-name": [true, "never-prefix"],
      "no-empty-interface": true,
      "object-literal-sort-keys": false
    },
    "rulesDirectory": []
  }
  